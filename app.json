{
  "expo": {
    "name": "Planner",
    "slug": "planner",
    "scheme": "planner",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#000000"
        }
      ]
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yourname.planner",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "We use notifications to remind you of tasks."
      }
    },
    "android": {
      "package": "com.yourname.planner",
      "permissions": [
        "POST_NOTIFICATIONS"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "b9dea583-4ad9-46b0-b884-03f07dc3dfd1"
      }
    }
  }
}
